{
  "boot-source": {
    "kernel_image_path": "./infra/vmlinux.bin",
    "boot_args": "console=ttyS0 reboot=k panic=1 pci=off quiet"
  },
  "drives": [
    {
      "drive_id": "rootfs",
      "path_on_host": "./infra/rootfs.ext4",
      "is_root_device": true,
      "is_read_only": false
    }
  ],
  "machine-config": {
    "vcpu_count": 2,
    "mem_size_mib": 512,
    "smt": false,
    "track_dirty_pages": true
  },
  "vsock": {
    "guest_cid": 3,
    "uds_path": "./v.sock"
  },
  "network-interfaces": []
}
